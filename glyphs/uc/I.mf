% box dimension definition %
beginadjchar("I",1.5,cap#,0);

% sidebearings %
numeric l,r;
normal_adjust_fit(space*0.1u#,space*0.1u#);

% construction %
x1=x2=.5w;
y2=h;
y1=0;

% draw %
draw z1---z2;

% composite glyphs %
charanchortops_[charcode]=(.5w,h+1/6cap);

penlabels(range 0 thru 30);

endchar;